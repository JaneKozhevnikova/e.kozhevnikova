
import unittest
from unittest import TestCase
##from program import Lion

class TestString(unittest.TestCase):
    def test(self):
        self.assertMultiLineEqual(input("Who can be seen by the lion?\n"), "hunter")
        self.assertMultiLineEqual(input("Who can be seen by the lion?\n"), "antelope")
        self.assertMultiLineEqual(input("Who can be seen by the lion?\n"), "tree")
if __name__ == '__main__':
    unittest.main()
